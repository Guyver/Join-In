package IRGlancePackage;

import java.util.EventListener;
/**
 * Must be implemented by all classes that want to get and deal with the IR speed.
 * @author Santiago Hors Fraile
 */
public interface IRGlanceListener extends EventListener{

	/**
	 * Must be implemented to manage the SpeedEvents.
	 * @param se The new SpeedEvent received.
	 */
	public void speedUpdated(SpeedEvent se);
	
}
