package iservices;

import services.*;

import control.IListenerCommModule;

/**
 * Must be implemented by any class which wants to receive gesture events from the WiiMote
 * @author Santiago Hors Fraile
 */
public interface IWiiMoteGestureService extends IListenerCommModule{
	/**
	 * Must be implemented to manage the WiiMoteServiceGestureEvent received.
	 * @param se This is the WiiMoteServiceGestureEvent received.
	 */
	void gestureReceived(WiiMoteServiceGestureEvent se);

}
