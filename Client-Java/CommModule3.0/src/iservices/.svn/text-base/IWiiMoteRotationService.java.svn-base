package iservices;

import services.*;

import control.IListenerCommModule;

/**
 * Must be implemented by any class which wants to receive rotation events from the WiiMotionPlus
 * @author Santiago Hors Fraile
 */
public interface IWiiMoteRotationService extends IListenerCommModule {
	/**
	 * Must be implemented to manage the WiiMoteServiceRotationEvent received.
	 * @param se This is the WiiMoteServiceRotationEvent received.
	 */
	void rotationReceived(WiiMoteServiceRotationEvent se);

}
