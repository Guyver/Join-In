package iservices;

import services.*;

import control.IListenerCommModule;

/**
 * Must be implemented by any class which wants to receive IR events from the WiiMote
 * @author Santiago Hors Fraile
 */
public interface IWiiMoteIRService extends IListenerCommModule{
	/**
	 * Must be implemented to manage the WiiMoteServiceIREvent received.
	 * @param se This is the WiiMoteServiceIREvent received.
	 */
	void iRReceived(WiiMoteServiceIREvent se);

}
